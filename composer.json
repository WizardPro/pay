{
    "name": "wizard-pro/pay",
    "description": "fork from yansongda/pay and support alipay cert with @cuikangyi 's pr https://github.com/yansongda/pay/pull/288",
    "keywords": ["alipay", "wechat", "pay"],
    "type": "library",
    "support": {
        "issues": "https://github.com/yansongda/pay/issues",
        "source": "https://github.com/yansongda/pay"
    },
    "require": {
        "php": ">=7.1.3",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "yansongda/supports": "^2.0",
        "symfony/http-foundation": "^4.0",
        "symfony/event-dispatcher": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "huoxiaoqiang",
            "email": "huoxiaoqiang@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "WizardPro\\Pay\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WizardPro\\Pay\\Tests\\": "tests"
        }
    }
}
